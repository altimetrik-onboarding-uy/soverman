    <validationRules>
        <fullName>Amount</fullName>
        <active>true</active>
        <errorConditionFormula>Amount__c &gt; 5000</errorConditionFormula>
        <errorDisplayField>Amount__c</errorDisplayField>
        <errorMessage>The Amount cant be over 5000$</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_details1</fullName>
        <active>true</active>
        <errorConditionFormula>AND(IF( ISPICKVAL(  Job_Category__c , &apos;Other&apos; ) , ISBLANK(  Other_details__c  )=true, null ))</errorConditionFormula>
        <errorDisplayField>Other_details__c</errorDisplayField>
        <errorMessage>Details of your Job Category, required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_details2</fullName>
        <active>true</active>
        <errorConditionFormula>AND(IF( ISPICKVAL(  Job_Category__c , &apos;Junior&apos; ) , NOT(ISBLANK(  Other_details__c  )=true), null ))</errorConditionFormula>
        <errorDisplayField>Other_details__c</errorDisplayField>
        <errorMessage>Your Job Category is Junior, we dont need further details...but, thank you.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_details3</fullName>
        <active>true</active>
        <errorConditionFormula>AND(IF( ISPICKVAL(  Job_Category__c , &apos;Semi-Senior&apos; ) , NOT(ISBLANK(  Other_details__c  )=true), null ))</errorConditionFormula>
        <errorDisplayField>Other_details__c</errorDisplayField>
        <errorMessage>Your Job Category is Semi-Senior, we dont need further details...but, thank you.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_details4</fullName>
        <active>true</active>
        <errorConditionFormula>AND(IF( ISPICKVAL(  Job_Category__c , &apos;Senior&apos; ) , NOT(ISBLANK(  Other_details__c  )=true), null ))</errorConditionFormula>
        <errorDisplayField>Other_details__c</errorDisplayField>
        <errorMessage>Your Job Category is Senior, we dont need further details...but, thank you.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date</fullName>
        <active>true</active>
        <errorConditionFormula>Start_Date__c &gt;  End_date__c</errorConditionFormula>
        <errorMessage>Start Date cant be older than End Date</errorMessage>
    </validationRules>
